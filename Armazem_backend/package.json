{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "API Fastify com TypeScript e Swagger",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "rbac:sync": "tsx scripts/sync-rbac-redis.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "fastify",
    "typescript",
    "swagger",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@sinclair/typebox": "^0.34.38",
    "bcrypt": "^6.0.0",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "ioredis": "^5.7.0",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^7.0.5",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.11",
    "@types/node-telegram-bot-api": "^0.64.10",
    "@types/nodemailer": "^7.0.0",
    "jest": "^30.0.5",
    "jsonwebtoken": "^9.0.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typebox": "^0.0.1",
    "typescript": "^5.9.2"
  }
}
